{
  "name": "jon-babylon-nodejs-test",
  "version": "1.0.0",
  "type": "module",
  "description": "Node.js test suite for jon-babylon Docker image",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "tsc && esbuild src/index.js --bundle --platform=node --outfile=dist/bundle.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.202",
    "typescript": "^5.3.3",
    "esbuild": "^0.19.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1"
  }
}